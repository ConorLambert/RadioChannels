@model IEnumerable<RadioChannels.Models.Channel>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

@foreach (var item in Model)
{
    <div>
        <h4>Favourite</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @item.CurrentTrack
            </dt>
            <dd>
                @item.Name
            </dd>
            <dt>
                @item.Id
            </dt>
            <dd>
                @item.Playlist
            </dd>
        </dl>
    </div>
}


